<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresQrs" table="indicadores_qrs">
    <indexes>
      <index name="persona" columns="persona"/>
      <index name="proyecto" columns="proyecto"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="smallint" column="tipo" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="procesada" type="boolean" column="procesada" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="persona" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="persona" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
