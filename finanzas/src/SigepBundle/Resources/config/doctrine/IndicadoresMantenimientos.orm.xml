<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresMantenimientos" table="indicadores_mantenimientos">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
      <index name="equipo" columns="equipo"/>
      <index name="tipo" columns="tipo"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="realizado" type="boolean" column="realizado" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="equipo" target-entity="IndicadoresEquipos" fetch="LAZY">
      <join-columns>
        <join-column name="equipo" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tipo" target-entity="IndicadoresTiposMantenimiento" fetch="LAZY">
      <join-columns>
        <join-column name="tipo" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
