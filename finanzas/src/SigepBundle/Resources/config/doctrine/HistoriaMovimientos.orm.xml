<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\HistoriaMovimientos" table="historia_movimientos">
    <indexes>
      <index name="movimiento" columns="movimiento"/>
      <index name="usuario" columns="usuario"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="valorAnterior" type="bigint" column="valor_anterior" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="diferencia" type="bigint" column="diferencia" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="operacion" type="integer" column="operacion" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="codigoOperacion" type="integer" column="codigo_operacion" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="movimiento" target-entity="Movimientos" fetch="LAZY">
      <join-columns>
        <join-column name="movimiento" referenced-column-name="Codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="usuario" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="usuario" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
