<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresNumeroInformes" table="indicadores_numero_informes">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
    </indexes>
    <id name="mes" type="integer" column="mes"/>
    <id name="proyecto" association-key="true"/>
    <field name="anio" type="integer" column="anio" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="cantidad" type="integer" column="cantidad" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <one-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
