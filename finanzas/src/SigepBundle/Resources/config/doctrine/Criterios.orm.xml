<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Criterios" table="criterios">
    <indexes>
      <index name="FK__proyectos" columns="proyecto"/>
      <index name="FK_criterios_criterios" columns="padre"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="habilitado" type="string" column="habilitado" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="padre" target-entity="Criterios" fetch="LAZY">
      <join-columns>
        <join-column name="padre" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
