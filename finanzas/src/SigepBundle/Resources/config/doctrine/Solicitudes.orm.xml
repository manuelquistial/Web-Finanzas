<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Solicitudes" table="solicitudes">
    <indexes>
      <index name="responsable" columns="responsable"/>
      <index name="solicitante" columns="solicitante"/>
      <index name="tipo" columns="tipo"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="asunto" type="string" column="asunto" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechasolicitud" type="date" column="fechaSolicitud" nullable="false"/>
    <field name="fechacierre" type="date" column="fechaCierre" nullable="true"/>
    <field name="estado" type="string" column="estado" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="horasolicitud" type="time" column="horaSolicitud" nullable="false"/>
    <field name="horacierre" type="time" column="horaCierre" nullable="true"/>
    <many-to-one field="responsable" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="responsable" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="solicitante" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="solicitante" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tipo" target-entity="TiposSolicitud" fetch="LAZY">
      <join-columns>
        <join-column name="tipo" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
