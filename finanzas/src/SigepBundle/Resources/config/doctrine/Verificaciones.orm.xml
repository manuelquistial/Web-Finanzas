<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Verificaciones" table="verificaciones">
    <indexes>
      <index name="FK_verificaciones_personas" columns="realizacion"/>
      <index name="IDX_86762ABDD7F21435" columns="meta"/>
    </indexes>
    <id name="mes" type="boolean" column="mes"/>
    <id name="meta" association-key="true"/>
    <field name="anio" type="smallint" column="anio" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="cumplimiento" type="float" column="cumplimiento" precision="5" scale="2" nullable="false">
      <options>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="observaciones" type="string" column="observaciones" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="limitaciones" type="string" column="limitaciones" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="conclusiones" type="string" column="conclusiones" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recomendaciones" type="string" column="recomendaciones" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="meta" target-entity="Metas" fetch="LAZY">
      <join-columns>
        <join-column name="meta" referenced-column-name="codigo"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="realizacion" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="realizacion" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
