<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\GruposPersona" table="grupos_persona">
    <indexes>
      <index name="persona" columns="persona"/>
      <index name="rol" columns="rol"/>
      <index name="IDX_98519948C0E9BD3" columns="grupo"/>
    </indexes>
    <id name="grupo" association-key="true"/>
    <id name="persona" association-key="true"/>
    <id name="rol" association-key="true"/>
    <one-to-one field="grupo" target-entity="Grupos" fetch="LAZY">
      <join-columns>
        <join-column name="grupo" referenced-column-name="codigo"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="persona" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="persona" referenced-column-name="Identificacion"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="rol" target-entity="Roles" fetch="LAZY">
      <join-columns>
        <join-column name="rol" referenced-column-name="Codigo"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
