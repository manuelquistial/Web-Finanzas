<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresAspectos" table="indicadores_aspectos">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="habilitado" type="boolean" column="habilitado" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="encuesta" target-entity="IndicadoresEncuestaPs" inversed-by="aspecto" fetch="LAZY">
      <join-table name="indicadores_calificaciones_encuesta_calidad">
        <join-columns>
          <join-column name="aspecto" referenced-column-name="codigo"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="encuesta" referenced-column-name="codigo"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
