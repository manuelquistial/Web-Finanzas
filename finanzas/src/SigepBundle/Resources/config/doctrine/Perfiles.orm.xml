<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Perfiles" table="perfiles">
    <indexes>
      <index name="dependencia" columns="dependencia"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="acceso" type="integer" column="acceso" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="where" type="string" column="where" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="dependencia" target-entity="Dependencias" fetch="LAZY">
      <join-columns>
        <join-column name="dependencia" referenced-column-name="Codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="destino" target-entity="Destinos" inversed-by="perfil" fetch="LAZY">
      <join-table name="alcances_perfil">
        <join-columns>
          <join-column name="perfil" referenced-column-name="codigo"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="destino" referenced-column-name="codigo"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="usuario" target-entity="Usuarios" mapped-by="perfil" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
