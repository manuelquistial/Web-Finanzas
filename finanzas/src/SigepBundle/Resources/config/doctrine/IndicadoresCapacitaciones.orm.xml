<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresCapacitaciones" table="indicadores_capacitaciones">
    <indexes>
      <index name="curso" columns="curso"/>
      <index name="facilitador" columns="facilitador"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="hora" type="time" column="hora" nullable="false"/>
    <field name="intensidad" type="integer" column="intensidad" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="lugar" type="string" column="lugar" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="curso" target-entity="IndicadoresCursos" fetch="LAZY">
      <join-columns>
        <join-column name="curso" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="facilitador" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="facilitador" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="participante" target-entity="Personas" inversed-by="capacitacion" fetch="LAZY">
      <join-table name="indicadores_participantes_capacitacion">
        <join-columns>
          <join-column name="capacitacion" referenced-column-name="codigo"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="participante" referenced-column-name="Identificacion"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
