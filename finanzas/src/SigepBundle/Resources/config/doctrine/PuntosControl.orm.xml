<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\PuntosControl" table="puntos_control">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
      <index name="compromiso" columns="compromiso"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="hora" type="time" column="hora" nullable="true"/>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fechaalerta" type="date" column="fechaAlerta" nullable="true"/>
    <field name="asunto" type="string" column="asunto" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="compromiso" target-entity="Compromisos" fetch="LAZY">
      <join-columns>
        <join-column name="compromiso" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
