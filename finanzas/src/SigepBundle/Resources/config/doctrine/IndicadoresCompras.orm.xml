<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\IndicadoresCompras" table="indicadores_compras">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="insumo" type="string" column="insumo" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaSolicitud" type="date" column="fecha_solicitud" nullable="false"/>
    <field name="mesEsperado" type="integer" column="mes_esperado" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="anioEsperado" type="integer" column="anio_esperado" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fechaRecepcion" type="date" column="fecha_recepcion" nullable="true"/>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
