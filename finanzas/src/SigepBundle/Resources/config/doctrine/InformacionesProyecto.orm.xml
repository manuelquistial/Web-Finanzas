<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\InformacionesProyecto" table="informaciones_proyecto">
    <indexes>
      <index name="proyecto" columns="proyecto"/>
      <index name="financiador" columns="financiador"/>
      <index name="codigo_operacion" columns="codigo_operacion"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="integer" column="tipo" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="inicio" type="date" column="inicio" nullable="true"/>
    <field name="fin" type="date" column="fin" nullable="true"/>
    <field name="valor" type="bigint" column="valor" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="contrato" type="string" column="contrato" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="objeto" type="text" column="objeto" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoOperacion" type="integer" column="codigo_operacion" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="proyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="proyecto" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="financiador" target-entity="Personas" fetch="LAZY">
      <join-columns>
        <join-column name="financiador" referenced-column-name="Identificacion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
