<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Parametros" table="parametros">
    <id name="parametro" type="string" column="parametro" length="30">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="valorNumerico" type="integer" column="valor_numerico" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="valorString" type="string" column="valor_string" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
