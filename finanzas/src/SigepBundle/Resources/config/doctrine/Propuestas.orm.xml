<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Propuestas" table="propuestas">
    <indexes>
      <index name="FK__oportunidades" columns="oportunidad"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aprobada" type="string" column="aprobada" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="causaNoAprobacion" type="text" column="causa_no_aprobacion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="presentada" type="string" column="presentada" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="causaNoPresentacion" type="text" column="causa_no_presentacion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valor" type="integer" column="valor" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registro" type="date" column="registro" nullable="false"/>
    <field name="entregacentro" type="date" column="entregaCentro" nullable="true"/>
    <field name="entregaentidad" type="date" column="entregaEntidad" nullable="true"/>
    <field name="observaciones" type="text" column="observaciones" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="oportunidad" target-entity="Oportunidades" fetch="LAZY">
      <join-columns>
        <join-column name="oportunidad" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="persona" target-entity="Personas" mapped-by="propuesta" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
