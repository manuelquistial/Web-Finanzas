<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\CentrosCosto" table="centros_costo">
    <id name="codigo" type="integer" column="Codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="Descripcion" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ordenador" type="string" column="Ordenador" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="usuario" target-entity="Usuarios" mapped-by="centroCosto" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
