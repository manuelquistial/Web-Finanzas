<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Listados" table="listados">
    <indexes>
      <index name="dependencia" columns="dependencia"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="200" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <many-to-one field="dependencia" target-entity="Dependencias" fetch="LAZY">
      <join-columns>
        <join-column name="dependencia" referenced-column-name="Codigo"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="movimiento" target-entity="Movimientos" inversed-by="listado" fetch="LAZY">
      <join-table name="movimientos_listado">
        <join-columns>
          <join-column name="listado" referenced-column-name="codigo"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="movimiento" referenced-column-name="Codigo"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
