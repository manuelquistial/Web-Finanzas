<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SigepBundle\Entity\Metas" table="metas">
    <indexes>
      <index name="FK_metas_compromisos" columns="compromiso"/>
    </indexes>
    <id name="codigo" type="integer" column="codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="text" column="nombre" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="indicador" type="string" column="indicador" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="compromiso" target-entity="Compromisos" fetch="LAZY">
      <join-columns>
        <join-column name="compromiso" referenced-column-name="codigo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
