<?php

namespace SigepBundle\Entity;

/**
 * Parametros
 */
class Parametros
{
    /**
     * @var integer
     */
    private $valorNumerico;

    /**
     * @var string
     */
    private $valorString;

    /**
     * @var string
     */
    private $parametro;


    /**
     * Set valorNumerico
     *
     * @param integer $valorNumerico
     *
     * @return Parametros
     */
    public function setValorNumerico($valorNumerico)
    {
        $this->valorNumerico = $valorNumerico;

        return $this;
    }

    /**
     * Get valorNumerico
     *
     * @return integer
     */
    public function getValorNumerico()
    {
        return $this->valorNumerico;
    }

    /**
     * Set valorString
     *
     * @param string $valorString
     *
     * @return Parametros
     */
    public function setValorString($valorString)
    {
        $this->valorString = $valorString;

        return $this;
    }

    /**
     * Get valorString
     *
     * @return string
     */
    public function getValorString()
    {
        return $this->valorString;
    }

    /**
     * Get parametro
     *
     * @return string
     */
    public function getParametro()
    {
        return $this->parametro;
    }
}
