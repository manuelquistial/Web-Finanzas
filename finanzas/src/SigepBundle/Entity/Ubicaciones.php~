<?php

namespace SigepBundle\Entity;

/**
 * Ubicaciones
 */
class Ubicaciones
{
    /**
     * @var string
     */
    private $nombre;

    /**
     * @var integer
     */
    private $codigo;

    /**
     * @var \SigepBundle\Entity\Ubicaciones
     */
    private $padre;


    /**
     * Set nombre
     *
     * @param string $nombre
     *
     * @return Ubicaciones
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }

    /**
     * Get nombre
     *
     * @return string
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Get codigo
     *
     * @return integer
     */
    public function getCodigo()
    {
        return $this->codigo;
    }

    /**
     * Set padre
     *
     * @param \SigepBundle\Entity\Ubicaciones $padre
     *
     * @return Ubicaciones
     */
    public function setPadre(\SigepBundle\Entity\Ubicaciones $padre = null)
    {
        $this->padre = $padre;

        return $this;
    }

    /**
     * Get padre
     *
     * @return \SigepBundle\Entity\Ubicaciones
     */
    public function getPadre()
    {
        return $this->padre;
    }
}
